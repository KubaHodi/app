<% if alert.present? %>
    <p class="alert"><%=alert%></p>
<%end%>

<% content_for :title, "Users" %>

<h1 class="text-center" >Users</h1>

<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", user %>
    </p>
  <% end %>
</div>

<%= link_to "New user", new_user_path %>
